
epm_flow:	epm_flow.tex
	pdflatex -record -interaction nonstopmode \
	         -no-shell-escape epm_flow.tex
	pdftocairo -jpeg -singlefile \
	           epm_flow.pdf epm_flow

epm_user:	epm_user.tex
	pdflatex -record -interaction nonstopmode \
	         -no-shell-escape epm_user.tex

epm_maintain:	epm_maintain.tex
	pdflatex -record -interaction nonstopmode \
	         -no-shell-escape epm_maintain.tex
