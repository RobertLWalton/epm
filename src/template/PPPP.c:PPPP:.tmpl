{

"COMMANDS": ["gcc AAAA -o PPPP PPPP.c BBBB > PPPP.out 2> PPPP.err"],
"LOCAL-REQUIRES":["PPPP.c"],
"KEEP":    ["PPPP"],
"SHOW":    ["PPPP.err","PPPP.out"],
"CHECKS":  ["cmp -s PPPP.err /dev/null",
            "cmp -s PPPP.out /dev/null"],
"OPTIONS": {
    "AAAA": { "opt": ["-Og", "-O3", ""],
	      "ped": ["", "-Wpedantic"]},
    "BBBB": { "gsl": ["", "-lgsl"],
              "crypto": ["", "-lcrypto"]}}

}
