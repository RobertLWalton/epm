
{

"COMMANDS": ["chmod o+r PPPP.pyc",
             "epm_sandbox GSARGS GSPACE generate_PPPP \\",
	     "    < XXXX-PPPP.in 2> XXXX-PPPP.gerr | \\",
	     "  epm_sandbox SSARGS SSPACE \\",
	     "    python3 PPPP.pyc DARGS  \\",
	     "    > XXXX-PPPP.sout 2> XXXX-PPPP.serr",
	     "chmod o-r PPPP.pyc"],

"LOCAL-REQUIRES": ["XXXX-PPPP.in", "generate_PPPP", "PPPP.pyc"],
"CREATABLE": ["generate_PPPP"],
"KEEP":    ["XXXX-PPPP.sout"],
"SHOW":    ["XXXX-PPPP.gerr", "XXXX-PPPP.serr", "XXXX-PPPP.sout"],
"CHECKS":  ["cmp -s XXXX-PPPP.gerr /dev/null",
            "cmp -s XXXX-PPPP.serr /dev/null"]

}

