{

"COMMANDS": ["epm_sandbox GSARGS GSPACE \\",
             "    -status XXXX-PPPP.gstat generate_PPPP \\",
             "    < XXXX-PPPP.in 2> XXXX-PPPP.gerr | \\",
	     "  epm_sandbox SSARGS SSTIME SSPACE \\",
	     "    -status XXXX-PPPP.sstat PPPP DARGS \\",
	     "    > XXXX-PPPP.sout 2> XXXX-PPPP.serr"],

"CONDITION": "UPLOAD XXXX-PPPP.in",
"LOCAL-REQUIRES":["PPPP"],
"REQUIRES":["generate_PPPP"],
"CREATABLE": ["generate_PPPP"],
"KEEP":    ["XXXX-PPPP.in", "XXXX-PPPP.sout"],
"SHOW":    ["XXXX-PPPP.gerr","XXXX-PPPP.serr", "XXXX-PPPP.sout"]

}
