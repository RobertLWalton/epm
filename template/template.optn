     "gtime": { "type": "natural",
                  "range": ["1", "30"],
		  "default": "3",
		  "description": "generate allowed CPU time in seconds",
		  "valname": "GTIME" },
     "gtime-arg": { "values": ["-cputime GTIME"],
		      "argname": "GSARGS" },
     "gvmem":  { "type": "natural",
                  "range": ["16", "2048"],
		  "default": "256",
		  "description": "generate non-JAVA allowed virtual memory in megabytes",
		 "valname": "GVMEM" },
     "gvmem-arg":  { "values": ["-space GVMEMm"],
		      "argname": "GSPACE" },
     "gfsize": { "type": "natural",
                   "range": ["1", "2048"],
		   "default": "128",
		   "description": "generate allowed output files size in kilobytes",
		   "valname": "GFSIZE" },
     "gfsize-arg": { "values": ["-filesize GFSIZEk"],
		        "argname": "GSARGS" },

     "stime": { "type": "natural",
                  "range": ["1", "30"],
		  "default": "3",
		  "description": "solution allowed CPU time in seconds",
		  "valname": "STIME" },
     "stime-arg": { "values": ["-SIGTERM STIME",
     				// -SIGINT will not work as
				// interrupts are disabled
				// when shell runs commands
                               "-cputime STIME"],
		      "argname": "SSARGS" },
     "svmem":  { "type": "natural",
                  "range": ["16", "2048"],
		  "default": "256",
		  "description": "solution's allowed virtual memory in megabytes",
		 "valname": "SVMEM" },
