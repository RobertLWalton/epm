		   "valname": "FFSIZE" },
     "ffsize-arg": { "values": ["-filesize FFSIZEk"],
		        "argname": "FSARGS" },

     "debug": { "type": "args",
		"argname": "DARGS" },

     "java-gc": { "values": ["-XX:+UseSerialGC"],
                  "argname": "JFLAGS" },
     "java-ss": { "type": "natural",
                  "range": ["4","64"],
		  "default": "32",
		  "description": "JAVA stack size in megabytes",
                  "valname": "STACK-SIZE" },
     "java-ss-arg": { "values": ["-XssSTACK-SIZEm"],
                      "argname": "JFLAGS" },
     "java-ms": { "type": "natural",
