		      "argname": "SSPACE" },
     "sfsize": { "type": "natural",
                   "range": ["1", "2048"],
		   "default": "128",
		   "description": "allowed solution output files size in kilobytes",
		   "valname": "SFSIZE" },
     "sfsize-arg": { "values": ["-filesize SFSIZEk"],
		        "argname": "SSARGS" },

     "ftime": { "type": "natural",
                  "range": ["1", "30"],
		  "default": "3",
		  "description": "filter allowed CPU time in seconds",
		  "valname": "FTIME" },
     "ftime-arg": { "values": ["-cputime FTIME"],
		      "argname": "FSARGS" },
     "fvmem":  { "type": "natural",
                  "range": ["16", "2048"],
		  "default": "256",
		  "description": "filter's allowed virtual memory in megabytes",
		 "valname": "FVMEM" },
     "fvmem-arg":  { "values": ["-space FVMEMm"],
		      "argname": "FSPACE" },
     "ffsize": { "type": "natural",
                   "range": ["1", "2048"],
